{
  "comments": {
    "lineComment": "#",
    "blockComment": [
      "/*",
      "*/"
    ]
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\""
    }
  ],
  "surroundingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\""
    }
  ],
  "indentationRules": {
    "increaseIndentPattern": "^.*\\{[^}]*$|^.*\\[[^\\]]*$|^.*\\([^)]*$",
    "decreaseIndentPattern": "^\\s*\\}|^\\s*\\]|^\\s*\\)"
  },
  "folding": {
    "markers": {
      "start": "^\\s*#region",
      "end": "^\\s*#endregion"
    },
    "offSide": true
  }
}

